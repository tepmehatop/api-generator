{
  "name": "@your-company/api-codegen",
  "version": "1.0.0",
  "description": "Advanced OpenAPI to TypeScript API client generator with automatic test generation and database integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./test-api": {
      "types": "./dist/test-api/index.d.ts",
      "default": "./dist/test-api/index.js"
    },
    "./test-api/*": {
      "types": "./dist/test-api/*.d.ts",
      "default": "./dist/test-api/*.js"
    }
  },
  "bin": {
    "api-codegen": "./bin/cli.js"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "README.md",
    "LICENSE",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsc",
    "build:api": "tsc --project tsconfig.api.json",
    "dev": "tsc --watch",
    "test": "node dist/example.js",
    "prepublishOnly": "npm run build && npm run update-exports && npm run build:api",
    "clean": "rm -rf dist",
    "generate": "node scripts/generate.js",
    "generate:help": "node scripts/generate.js --help",
    "update-exports": "node scripts/update-exports.cjs",
    "postgenerate": "npm run update-exports && npm run build:api"
  },
  "keywords": [
    "openapi",
    "swagger",
    "typescript",
    "api",
    "generator",
    "codegen",
    "playwright",
    "testing",
    "test-automation",
    "database-analyzer",
    "api-client"
  ],
  "author": "Your Company",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-company/api-codegen.git"
  },
  "bugs": {
    "url": "https://github.com/your-company/api-codegen/issues"
  },
  "homepage": "https://github.com/your-company/api-codegen#readme",
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "axios": "^1.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://your-internal-npm-registry.com/"
  }
}
